.container {
  position: fixed;
  width: 100%;
  height: 100%;
  background-image: url("https://pikacode.oss-cn-chengdu.aliyuncs.com/school_cat/house_bg_z.png");
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.container .header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
}
.container .header .center {
  font-size: 25rpx;
  font-family: "Adobe Heiti Std";
  color: rgba(251, 251, 251, 0.412);
  line-height: 2.841;
  -moz-transform: matrix(1.7359549983, -0.0094755626, 0.0094755626, 1.7359549983, 0, 0);
  -webkit-transform: matrix(1.7359549983, -0.0094755626, 0.0094755626, 1.7359549983, 0, 0);
  -ms-transform: matrix(1.7359549983, -0.0094755626, 0.0094755626, 1.7359549983, 0, 0);
}
.container .header .right {
  width: 80rpx;
}
.container .map_box {
  height: 500rpx;
  box-sizing: border-box;
  border: 18rpx solid #370d6f;
}
.container .map_box .map {
  width: 100%;
  height: 100%;
}
.container .chat_box {
  position: relative;
  display: flex;
  flex-direction: column;
}
.container .chat_box .voice-mask {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 9;
  background-color: rgba(0, 0, 0, 0.6);
}
.container .chat_box .voice-mask .voice-bar {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -30%);
  padding: 25rpx;
  background-position: 50rpx;
  border-radius: 26rpx;
  margin-bottom: 200rpx;
  transition: all ease 0.5s;
}
.container .chat_box .voice-mask .voice-bar .icon {
  width: 100%;
  height: 50rpx;
  background-image: url(https://pikacode.oss-cn-chengdu.aliyuncs.com/school_cat/sonic_wave.png);
  background-repeat: repeat-x;
  background-size: 50rpx;
}
.container .chat_box .voice-mask .send-tip {
  position: absolute;
  left: 50%;
  bottom: 150rpx;
  transform: translateX(-50%);
  color: #bfbfbf;
}
.container .chat_box .voice-mask .mask-bottom {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  border-top: #BABABB 8rpx solid;
  border-radius: 100rpx 100rpx 0 0;
  background-image: linear-gradient(#949794, #e1e3e1);
}
.container .chat_box .view {
  flex: 1;
  overflow: auto;
  padding-bottom: 20rpx;
  margin-bottom: 80rpx;
  box-sizing: border-box;
}
.container .chat_box .view ::-webkit-scrollbar {
  display: none;
}
.container .chat_box .view .iten {
  overflow: auto;
  margin: 30rpx 18rpx;
}
.container .chat_box .view .iten .avatar {
  width: 80rpx;
  height: 80rpx;
  border-radius: 15rpx;
  background-color: pink;
  overflow: hidden;
}
.container .chat_box .view .iten .avatar .image {
  width: 100%;
  height: 100%;
}
.container .chat_box .view .iten .content {
  position: relative;
  max-width: 400rpx;
  margin: 5rpx 30rpx;
  padding: 15rpx 20rpx;
  background-color: #fff;
  border-radius: 8rpx;
  word-wrap: break-word;
}
.container .chat_box .view .iten .voice {
  height: 40rpx;
}
.container .chat_box .view .iten .voice .icon {
  width: 100%;
  height: 50rpx;
  background-image: url(https://pikacode.oss-cn-chengdu.aliyuncs.com/school_cat/sonic_wave.png);
  background-repeat: repeat-x;
  background-size: 40rpx;
}
.container .chat_box .view .other .avatar {
  float: left;
}
.container .chat_box .view .other .content {
  float: left;
}
.container .chat_box .view .other .content::before {
  content: "";
  display: block;
  position: absolute;
  top: 50%;
  left: 0;
  width: 10rpx;
  height: 10rpx;
  transform: translate(-50%, -50%) rotate(45deg);
  background-color: #fff;
}
.container .chat_box .view .me .avatar {
  float: right;
}
.container .chat_box .view .me .content {
  float: right;
}
.container .chat_box .view .me .content::after {
  content: "";
  display: block;
  position: absolute;
  top: 50%;
  right: 0;
  width: 10rpx;
  height: 10rpx;
  transform: translate(50%, -50%) rotate(45deg);
  background-color: #fff;
}
.container .chat_box .input {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  height: 80rpx;
  background-color: #e7e3e8;
}
.container .chat_box .input .item {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 80rpx;
  height: 80rpx;
}
.container .chat_box .input .center {
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
  margin: 10rpx;
  border-radius: 15rpx;
  background-color: #fff;
}
.container .chat_box .input .center .message {
  width: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
}
.container .chat_box .input .center .text {
  font-size: 30rpx;
  font-family: "SimHei";
  color: black;
}
.container .chat_box .input .right {
  display: flex;
}
.container .chat_box .input .right .btn {
  display: flex;
  align-items: center;
  margin: 0 5rpx;
}